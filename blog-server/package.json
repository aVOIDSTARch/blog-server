{
  "name": "@casinelli/blog-server",
  "version": "1.0.1",
  "type": "module",
  "description": "A RESTful API server for managing multi-tenant blog sites with Express, Prisma, and PostgreSQL",
  "author": "Louis Casinelli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/casinelli/blog-server.git"
  },
  "bugs": {
    "url": "https://github.com/casinelli/blog-server/issues"
  },
  "homepage": "https://github.com/casinelli/blog-server#readme",
  "keywords": [
    "blog",
    "api",
    "express",
    "prisma",
    "postgresql",
    "multi-tenant",
    "rest",
    "cms"
  ],
  "main": "api/server.ts",
  "bin": {
    "blog-server": "./bin/cli.js"
  },
  "files": [
    "api",
    "src",
    "prisma",
    "bin",
    "docs",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx watch api/server.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start:api": "tsx api/server.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:init": "tsx scripts/init-db.ts",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "supertest": "^7.1.4",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@solidjs/router": "^0.15.4",
    "@supabase/supabase-js": "^2.89.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "solid-js": "^1.9.10",
    "swagger-ui-express": "^5.0.1",
    "yaml": "^2.8.2"
  }
}
